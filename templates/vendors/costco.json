{
  "vendor_id": "costco",
  "vendor_name": "Costco Wholesale",
  "recognition_patterns": [
    "costco",
    "costco wholesale",
    "warehouse"
  ],
  "header_patterns": {
    "store_name": "costco\\s+wholesale",
    "store_number": "warehouse\\s*#?\\s*(\\d+)",
    "membership_number": "member\\s*#?\\s*(\\d+)"
  },
  "item_pattern": "^(.+?)\\s+(?:\\d+)\\s+(\\$?\\d+\\.\\d{2})\\s*$",
  "total_pattern": "(?:total|amount paid)\\s*(?:\\w+\\s+)?(?:\\w+\\s+)?[:\\*]?\\s*(\\$?\\d+\\.\\d{2})",
  "subtotal_pattern": "subtotal\\s*(?:\\w+\\s+)?[:\\*]?\\s*(\\$?\\d+\\.\\d{2})",
  "tax_pattern": "(?:sales tax|tax)\\s*(?:\\w+\\s+)?[:\\*]?\\s*(\\$?\\d+\\.\\d{2})",
  "date_pattern": "(?:\\d{1,2}/\\d{1,2}/\\d{2,4})",
  "time_pattern": "(?:\\d{1,2}:\\d{2}(?::\\d{2})?(?:\\s*[AP]M)?)",
  "receipt_number_pattern": "(?:receipt\\s*(?:#|no|number)[:\\s]?\\s*(\\w+))",
  "payment_method_pattern": "(?:(?:credit|debit|visa|mastercard|master card|amex|american express|discover|cash|ebt)[\\s\\w]*)",
  "address_pattern": "(\\d+\\s+[\\w\\s]+(?:road|rd|street|st|avenue|ave|boulevard|blvd|drive|dr|lane|ln|place|pl|way|parkway|pkwy)\\s*[.,]?\\s*[\\w\\s]+,\\s*[A-Za-z]{2}\\s*\\d{5}(?:-\\d{4})?)",
  "phone_pattern": "(\\(\\d{3}\\)\\s*\\d{3}-\\d{4}|\\d{3}-\\d{3}-\\d{4})"
} 